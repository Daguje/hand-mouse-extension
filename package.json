{
  "name": "chrome-extension-webpack",
  "version": "0.2.0",
  "description": "Get started with Chrome extensions development using webpack, Typescript, Sass, and more",
  "scripts": {
    "dev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js --target web",
    "build:zip": "yarn build && cd dist && zip -r ../extension.zip * && cd ..",
    "lint": "eslint --ext .ts,.js --max-warnings=0 .",
    "prettier": "prettier --write",
    "test": "jest --passWithNoTests"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/chrome": "^0.0.193",
    "@types/jest": "^29.5.12",
    "@types/toastify-js": "^1.12.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dotenv-webpack": "^8.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-webpack-plugin": "^3.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "postcss-preset-env": "^8.3.2",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.7.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@mediapipe/hands": "^0.4.1675469240",
    "@mediapipe/tasks-vision": "^0.10.9",
    "@tensorflow-models/hand-pose-detection": "^2.0.0",
    "@tensorflow/tfjs": "^4.17.0",
    "@tensorflow/tfjs-backend-wasm": "^4.17.0",
    "@tensorflow/tfjs-backend-webgl": "^4.4.0",
    "@tensorflow/tfjs-converter": "^4.4.0",
    "@tensorflow/tfjs-core": "^4.4.0",
    "fingerpose": "^0.1.0",
    "libsvm-js": "^0.2.1",
    "normalize.css": "^8.0.1",
    "toastify-js": "^1.12.0",
    "webextension-polyfill-ts": "^0.26.0"
  },
  "browserslist": "> 0.25%, not dead",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
